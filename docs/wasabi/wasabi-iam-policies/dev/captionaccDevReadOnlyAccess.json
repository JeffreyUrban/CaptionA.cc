{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListCaptionAccDevBucketOnly",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": "arn:aws:s3:::captionacc-dev"
    },
    {
      "Sid": "AllowReadOnlyCaptionAccDevObjects",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": "arn:aws:s3:::captionacc-dev/*"
    },
    {
      "Sid": "DenyAccessToAllOtherBuckets",
      "Effect": "Deny",
      "Action": "s3:*",
      "NotResource": [
        "arn:aws:s3:::captionacc-dev",
        "arn:aws:s3:::captionacc-dev/*"
      ]
    },
    {
      "Sid": "DenyListAllBuckets",
      "Effect": "Deny",
      "Action": [
        "s3:ListAllMyBuckets",
        "s3:GetBucketLocation"
      ],
      "Resource": "*"
    }
  ]
}